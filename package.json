{
  "name": "pebble",
  "private": true,
  "scripts": {
    "build": "babel App.jsx > App.js"
  },
  "dependencies": {
    "@kachang/pebble.js": "^0.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1"
  },
  "babel": {
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "Pebble.createElement",
          "pragmaFrag": null
        }
      ]
    ]
  }
}
